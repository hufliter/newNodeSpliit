{
  "restApiRoot": "/api",
  "host": "0.0.0.0",
  "port": 3000,
  "remoting": {
    "context": {
      "enableHttpContext": true
    },
    "rest": {
      "normalizeHttpPath": false,
      "xml": false
    },
    "json": {
      "strict": false,
      "limit": "100kb"
    },
    "urlencoded": {
      "extended": true,
      "limit": "100kb"
    },
    "cors": false,
    "errorHandler": {
      "disableStackTrace": false
    }
  },
  "legacyExplorer": false,
  "imageType": "large",
  "hashSalt": "sEcrEt",
  "NB_DEAL_BY_PAGE": 4,
  "URL_SITE": "http://localhost:3000",
  "HOME_URL": "http://localhost:9000/#"
}
